#: templates/account/mail/reset_password.html:2
#, python-format
msgid "You have requested password restore on %(domain)s."
msgstr "Ви здійснили запит щодо зміни паролю на %(domain)s."

#: templates/account/mail/reset_password.html:3
#, python-format
msgid "Click <a href=\"%(url)s\">this link</a> to change your password."
msgstr "Перейдіть <a href=\"%(url)s\">за цим посиланням</a> для того, щоб змінити свій пароль."

